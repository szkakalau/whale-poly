{
  "name": "whale-intelligence",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "prisma generate && NODE_OPTIONS='--max-old-space-size=4096' tsc -p .",
    "start": "node dist/src/index.js",
    "start:prod": "prisma migrate deploy && node dist/src/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:deploy": "prisma migrate deploy",
    "backtest": "ts-node src/backtest/whale_score_v1.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.9.1",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "axios": "^1.6.7",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "https-proxy-agent": "^7.0.6",
    "node-cron": "^3.0.2",
    "prisma": "^5.9.1",
    "stripe": "^20.2.0",
    "telegraf": "^4.16.3",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/node-cron": "^3.0.11",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0"
  }
}
